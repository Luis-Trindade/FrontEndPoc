<body class="login-background" >
<!-- Modal Adicionar Cliente-->

<div id="login" class="modal show" role="dialog">
    <div class="modal-dialog modal-sm">
        <!-- Modal content-->
        <div class="modal-content">

            <div class="modal-header modal-header-primary">
                <!-- <button type="button" class="close" data-dismiss="modal">&times;</button>-->
                <h4 class="modal-title ">Faça o seu Login</h4>
            </div>
            <div class="modal-body">
                <form class="form-horizontal">
                    <div class="form-group"></div> <!-- espaçamento -->
                    <div class="form-group has-feedback has-feedback-left">
                        <div class="col-md-10 col-md-offset-1">
                            <input class="form-control" id="username" placeholder="Utilizador">
                            <i class="glyphicon glyphicon-user form-control-feedback"></i>
                        </div>
                    </div>
                    <div class="form-group has-feedback has-feedback-left">
                        <!--<label class="control-label col-md-3" for="password">Password</label>-->
                        <div class="col-md-10 col-md-offset-1">
                            <input type="password" class="form-control" id="password" placeholder="Password">
                            <i class="glyphicon glyphicon-lock form-control-feedback"></i>
                        </div>
                    </div>
                    <div class="form-group">
                        <span class="col-md-1 col-sm-1 col-xs-1"></span>
                        <label class="checkbox-inline control-label" id="remember">
                            <div class="col-md-1 col-sm-1 col-xs-1"><input type="checkbox" value=""></div>Lembrar-me?</label>
                    </div>

                </form>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-info col-md-10 col-md-offset-1 col-sm-10 col-sm-offset-1 col-xs-10 col-xs-offset-1" data-dismiss="modal">Entrar</button>
                <!--<button type="button" class="btn btn-danger" data-dismiss="modal">Cancelar</button>-->
            </div>
        </div>

    </div>
</div>


<!-- /#wrapper -->

